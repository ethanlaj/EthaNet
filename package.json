{
  "name": "ethanet",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --ignore 'src/parser.js' --exec 'jison src/parser.jison -o src/parser.js && node src/index.js'",
    "build": "jison src/parser.jison -o src/parser.js && cp src/* web/copiedFromSrc && webpack --mode=production"
  },
  "dependencies": {
    "jison": "^0.4.18",
    "json-formatter-js": "^2.3.4",
    "path-browserify": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
